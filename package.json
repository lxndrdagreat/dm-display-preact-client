    "@snowpack/plugin-dotenv": "^2.0.5",
    "@snowpack/plugin-typescript": "^1.2.0",
    "@snowpack/web-test-runner-plugin": "^0.2.0",
    "@testing-library/preact": "^2.0.0",
    "@types/chai": "^4.2.15",
    "@types/classnames": "^2.2.11",
    "@types/react-redux": "^7.1.16",
    "@web/test-runner": "^0.12.15",
    "chai": "^4.3.0",
    "classnames": "^2.2.6",
    "clsx": "^1.1.1",
    "dotenv": "^8.2.0",
    "prettier": "^2.0.5",
